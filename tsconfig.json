{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ESNext",
    "typeRoots": [
      "node_modules/@types",
      "src/@types"
    ],
    "jsx": "preserve",
    "lib": [
      "dom",
      "esnext"
    ],
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "suppressExcessPropertyErrors": false
  },
  "exclude": [
    "node_modules",
    "public",
    ".cache"
  ]
}